<%= render '/admin/navigation' %>
<section class="container p-top17
">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <h3 class="m-bottom44
">Log in</h3>
          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <p><%= notice %></p>
          <p><%= alert %></p>
           <div class="form-group">
              <label for="email">Email</label>
              <%= f.email_field :email, autofocus: true, id:"email"%>
            </div>

           <div class="form-group">
              <label for="password">Password</label>
              <%= f.password_field :password, autocomplete: "off", id:"password"%>
            </div>

            <% if devise_mapping.rememberable? %>
              <div class="form-group">
                <div class="checkbox">
                  <%= f.check_box :remember_me, id: "remember-me"%>
                  <label for="remember-me">Remember me</label>
                </div>
              </div>
            <% end %>

          <div class="form-group clearfix">
            <%= button_tag "Log in", class: "btn btn-warning  m-p-top17
 pull-left" %>
          </div>
          <% end %>
          <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</section>

 <%= render "devise/shared/error_messages", resource: resource %>

