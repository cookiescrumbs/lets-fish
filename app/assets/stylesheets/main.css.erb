/* search */

body#search {
    background-color: #f7f7f7;
}

#search #map {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 500px;
    margin-top: 0px;
    margin-bottom: 10px;
}

#results-container div.result-item {
    background-color: transparent;
}

@media(min-width: 768px) {
    .sidebar {
        position: fixed;
        top: 70px;
        left: 0;
        bottom: 0;
        width: 60%;
        overflow-y: scroll;
        -webkit-overflow-scrolling: touch;
    }

    .map-container {
        display: block;
        width: 40%;
    }

    .map-container:before {
        content: '';
        position: absolute;
        height: 100%;
        right: 0;
        -webkit-box-shadow: inset -1px 0 1px rgba(0, 0, 0, 0.3);
        box-shadow: inset -1px 0 1px rgba(0, 0, 0, 0.3);
        width: 2px;
        z-index: 2;
    }

    .map-container {
        position: fixed;
        right: 0;
        left: auto;
        bottom: 0;
        width: 100%;
        top: 75px;
    }

    #search #map {
        position: fixed;
        right: 0;
        left: auto;
        width: 40%;
        height: 100%;
        margin-top: 0px;
        margin-bottom: 10px;
        float: right;
        z-index: 0;
    }

    #search footer {
        display: none;
    }
}

